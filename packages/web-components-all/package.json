{
  "name": "@ootmm/web-components-all",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "tsx ./build.ts prod",
    "build:dev": "tsx ./build.ts dev"
  },
  "main": "dist/index.js",
  "author": "Nax",
  "license": "MIT",
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "{projectRoot}/**/*"
        ],
        "outputs": [
          "{projectRoot}/dist/**/*"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "build:dev": {
        "inputs": [
          "{projectRoot}/**/*"
        ],
        "outputs": [
          "{projectRoot}/dist/**/*"
        ],
        "dependsOn": [
          "^build",
          { "projects": ["@ootmm/web-components"], "target": "build" }
        ]
      }
    }
  }
}
